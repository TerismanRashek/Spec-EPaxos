#!/bin/bash

TLCDIR="/home/terisman/Projet_ASR_SPEC/tlaplus/tlatools/org.lamport.tlatools"
JARGS="-cp $/home/terisman/Projet_ASR_SPEC/tlaplus/tla2tools.jar -XX:+UseParallelGC tlc2.TLC"
ARGS="-workers $(nproc) -cleanup"

MODULES="SMRspec"


for mod in ${MODULES};
do
    java ${JARGS} ${ARGS} ${mod}
done
